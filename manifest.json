{
    "manifest_version": 3,
    "name": "Authentic Search",
    "description": "Get only 1 result per site",
    "version": "1.0.0",
    "permissions": ["storage", "tabs", "scripting", "activeTab"],
    "host_permissions": ["https://*.google.com/*"],
    "action":{
        "default_popup": "index.html"
    },
    "background": {
        "service_worker": "script.js"
    }
}